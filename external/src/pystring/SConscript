Import('env', 'os', 'utils')

script_dir = os.path.dirname(File('SConscript').rfile().abspath)

lib_env = env.Clone()
lib_env.Append(CPPPATH = [os.path.join(script_dir, 'pystring'), script_dir])

lib = lib_env.Library('pystring', 'pystring/pystring.cpp')

Return('lib_env', 'lib')

